# Database (Supabase PostgreSQL)
# Get this from: Supabase Dashboard > Project Settings > Database > Connection String (URI)
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres"

# Clerk Authentication
# Get these from: https://dashboard.clerk.com > Your App > API Keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="pk_test_..."
CLERK_SECRET_KEY="sk_test_..."

# Clerk URLs (customize sign-in/sign-up paths)
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/dashboard

# Clerk Webhook Secret (for syncing users to database)
# Get this from: Clerk Dashboard > Webhooks > Signing Secret
CLERK_WEBHOOK_SECRET="whsec_..."

# Google Gemini AI API
# Get this from: https://aistudio.google.com/apikey
GEMINI_API_KEY="your-gemini-api-key"
